<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="http://getlime.io/security/powerauth/v3"
           targetNamespace="http://getlime.io/security/powerauth/v3"
           elementFormDefault="qualified">

    <!-- Status method //-->

    <xs:element name="GetSystemStatusRequest">
        <xs:annotation>
            <xs:documentation>Request for getting the system status information.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence/>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetSystemStatusResponse">
        <xs:annotation>
            <xs:documentation>Response for getting the system status information.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="status" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationName" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationDisplayName" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationEnvironment" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="version" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="buildTime" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
                <xs:element name="timestamp" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Server error code list -->

    <xs:element name="GetErrorCodeListRequest">
        <xs:annotation>
            <xs:documentation>Request for getting the list of all possible server error codes.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="language" type="xs:string" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetErrorCodeListResponse">
        <xs:annotation>
            <xs:documentation>Response for getting the list of all possible server error codes.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="errors" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="code" type="xs:string" minOccurs="1" maxOccurs="1"/>
                            <xs:element name="value" type="xs:string" minOccurs="1" maxOccurs="1"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- List applications //-->

    <xs:element name="GetApplicationListRequest">
        <xs:annotation>
            <xs:documentation>Request for getting the application list.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence/>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetApplicationListResponse">
        <xs:annotation>
            <xs:documentation>Response for getting the application list.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applications" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                            <xs:element name="applicationRoles" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Get application details //-->

    <xs:element name="GetApplicationDetailRequest">
        <xs:annotation>
            <xs:documentation>Request for getting the application detail, including the list of versions.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationId" type="xs:string" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetApplicationDetailResponse">
        <xs:annotation>
            <xs:documentation>Response for getting the application detail, including the list of versions.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationRoles" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="masterPublicKey" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="versions" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="applicationVersionId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                            <xs:element name="applicationKey" type="xs:string" minOccurs="1" maxOccurs="1"/>
                            <xs:element name="applicationSecret" type="xs:string" minOccurs="1" maxOccurs="1"/>
                            <xs:element name="supported" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Get application details by application key //-->

    <xs:element name="LookupApplicationByAppKeyRequest">
        <xs:annotation>
            <xs:documentation>Request for getting the application detail based on the application version key.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationKey" type="xs:string" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="LookupApplicationByAppKeyResponse">
        <xs:annotation>
            <xs:documentation>Response for getting the application detail based on the application version key.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Create application //-->

    <xs:element name="CreateApplicationRequest">
        <xs:annotation>
            <xs:documentation>Request for creating a new activation.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="CreateApplicationResponse">
        <xs:annotation>
            <xs:documentation>Response for creating a new activation.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationRoles" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Create application version //-->

    <xs:element name="CreateApplicationVersionRequest">
        <xs:annotation>
            <xs:documentation>Request for creating a new activation version.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationVersionId" type="xs:string" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="CreateApplicationVersionResponse">
        <xs:annotation>
            <xs:documentation>Response for creating a new activation version.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationVersionId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationKey" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationSecret" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="supported" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Unsupport application version //-->

    <xs:element name="UnsupportApplicationVersionRequest">
        <xs:annotation>
            <xs:documentation>Request for marking a given application version as unsupported.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationVersionId" type="xs:string" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="UnsupportApplicationVersionResponse">
        <xs:annotation>
            <xs:documentation>Response for marking a given application version as unsupported.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationVersionId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="supported" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Support application version //-->

    <xs:element name="SupportApplicationVersionRequest">
        <xs:annotation>
            <xs:documentation>Request for marking a given application version as supported.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationVersionId" type="xs:string" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="SupportApplicationVersionResponse">
        <xs:annotation>
            <xs:documentation>Response for marking a given application version as supported.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationVersionId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="supported" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Init activation //-->

    <xs:element name="InitActivationRequest">
        <xs:annotation>
            <xs:documentation>Request for initiating the activation process by generating the activation code.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="userId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="timestampActivationExpire" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
                <xs:element name="maxFailureCount" type="xs:long" minOccurs="0" maxOccurs="1"/>
                <xs:element name="activationOtpValidation" type="tns:ActivationOtpValidation" minOccurs="0" maxOccurs="1"/>
                <xs:element name="activationOtp" type="xs:string" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="InitActivationResponse">
        <xs:annotation>
            <xs:documentation>Response for initiating the activation process.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="activationCode" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="activationSignature" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="userId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Exchange public keys //-->

    <xs:element name="PrepareActivationRequest">
        <xs:annotation>
            <xs:documentation>Request for client-server key exchange procedure.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationCode" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationKey" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="generateRecoveryCodes" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
                <xs:element name="ephemeralPublicKey" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="encryptedData" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="mac" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="nonce" type="xs:string" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="PrepareActivationResponse">
        <xs:annotation>
            <xs:documentation>Response for client-server key exchange procedure.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="userId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="encryptedData" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="mac" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="activationStatus" type="tns:ActivationStatus" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Create new activation directly //-->

    <xs:element name="CreateActivationRequest">
        <xs:annotation>
            <xs:documentation>Request for creating a new activation directly.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="userId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="timestampActivationExpire" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
                <xs:element name="generateRecoveryCodes" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
                <xs:element name="maxFailureCount" type="xs:long" minOccurs="0" maxOccurs="1"/>
                <xs:element name="applicationKey" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="ephemeralPublicKey" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="encryptedData" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="mac" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="nonce" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="activationOtp" type="xs:string" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="CreateActivationResponse">
        <xs:annotation>
            <xs:documentation>Response for creating a new activation directly.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="userId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="encryptedData" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="mac" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="activationStatus" type="tns:ActivationStatus" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Update activation OTP //-->

    <xs:element name="UpdateActivationOtpRequest">
        <xs:annotation>
            <xs:documentation>Request for update activation OTP before activation commit.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="externalUserId" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="activationOtp" type="xs:string" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="UpdateActivationOtpResponse">
        <xs:annotation>
            <xs:documentation>Response for update activation OTP before activation commit.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="updated" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Commit activation //-->

    <xs:element name="CommitActivationRequest">
        <xs:annotation>
            <xs:documentation>Request for committing the activation with given ID.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="externalUserId" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="activationOtp" type="xs:string" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="CommitActivationResponse">
        <xs:annotation>
            <xs:documentation>Response for committing the activation with given ID.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="activated" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Get Activation Status //-->

    <xs:element name="GetActivationStatusRequest">
        <xs:annotation>
            <xs:documentation>Request for getting the activation details / status information.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="challenge" type="xs:string" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetActivationStatusResponse">
        <xs:annotation>
            <xs:documentation>Response for getting the activation details / status information.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="activationStatus" type="tns:ActivationStatus" minOccurs="1" maxOccurs="1"/>
                <xs:element name="activationOtpValidation" type="tns:ActivationOtpValidation" minOccurs="1" maxOccurs="1"/>
                <xs:element name="blockedReason" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="activationName" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="userId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="extras" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="platform" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="deviceInfo" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="activationFlags" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="timestampCreated" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
                <xs:element name="timestampLastUsed" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
                <xs:element name="timestampLastChange" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
                <xs:element name="encryptedStatusBlob" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="encryptedStatusBlobNonce" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="activationCode" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="activationSignature" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="devicePublicKeyFingerprint" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="version" type="xs:long" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Remove Activation //-->

    <xs:element name="RemoveActivationRequest">
        <xs:annotation>
            <xs:documentation>Request for removing the activation with given ID.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="externalUserId" type="xs:string" minOccurs="0" maxOccurs="1"/>
                <xs:element name="revokeRecoveryCodes" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="RemoveActivationResponse">
        <xs:annotation>
            <xs:documentation>Response for removing the activation with given ID.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="removed" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Activations for user //-->

    <xs:element name="GetActivationListForUserRequest">
        <xs:annotation>
            <xs:documentation>Request for getting the list of activations for given user.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="userId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="applicationId" type="xs:string" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="GetActivationListForUserResponse">
        <xs:annotation>
            <xs:documentation>Response for getting the list of activations for given user.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="userId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                <xs:element name="activations" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="activationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                            <xs:element name="activationStatus" type="tns:ActivationStatus" minOccurs="1"
                                        maxOccurs="1"/>
                            <xs:element name="blockedReason" type="xs:string" minOccurs="0" maxOccurs="1"/>
                            <xs:element name="activationName" type="xs:string" minOccurs="0" maxOccurs="1"/>
                            <xs:element name="extras" type="xs:string" minOccurs="0" maxOccurs="1"/>
                            <xs:element name="platform" type="xs:string" minOccurs="0" maxOccurs="1"/>
                            <xs:element name="deviceInfo" type="xs:string" minOccurs="0" maxOccurs="1"/>
                            <xs:element name="activationFlags" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                            <xs:element name="timestampCreated" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
                            <xs:element name="timestampLastUsed" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
                            <xs:element name="timestampLastChange" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
                            <xs:element name="userId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                            <xs:element name="applicationId" type="xs:string" minOccurs="1" maxOccurs="1"/>
                            <xs:element name="version" type="xs:long" minOccurs="1" maxOccurs="1"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- Lookup activations //-->

    <xs:element name="LookupActivationsRequest">
        <xs:annotation>
            <xs:documentation>Request for querying activations using various parameters.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
